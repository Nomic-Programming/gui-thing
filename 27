-- Scripts:

local function SBOZ_fake_script() -- ShrekCCHat.LocalScript 
	local script = Instance.new('LocalScript', ShrekCCHat)

	local player = game.Players.LocalPlayer
	
	local function stopAnimations(humanoid, stop)
		for _, track in pairs(humanoid:GetPlayingAnimationTracks()) do
			if stop then
				track:Stop()
			else
				track:Play()
			end
		end
	end
	
	local function disableCollisionTemporarily(character)
		local originalStates = {}
	
		for _, descendant in pairs(character:GetDescendants()) do
			if descendant:IsA("BasePart") then
				originalStates[descendant] = descendant.CanCollide
				descendant.CanCollide = false
			end
		end
	
		return originalStates
	end
	
	local function restoreCollisions(states)
		for part, state in pairs(states) do
			if part and part.Parent then
				part.CanCollide = state
			end
		end
	end
	
	local function setupButton(scriptParent, character)
		local hrp = character:WaitForChild("HumanoidRootPart")
		local humanoid = character:WaitForChild("Humanoid")
	
		scriptParent.Activated:Connect(function()
			local originalCFrame = hrp.CFrame
			local originalCollisions = disableCollisionTemporarily(character)
	
			humanoid.PlatformStand = true
			stopAnimations(humanoid, true)
	
			for i = 1, 100 do
				hrp.CFrame = CFrame.new(-254, 1337, 444)
				task.wait(0.01)
			end
	
			restoreCollisions(originalCollisions)
			humanoid.PlatformStand = false
			stopAnimations(humanoid, false)
	
			hrp.CFrame = originalCFrame
		end)
	end
	
	if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
		setupButton(script.Parent, player.Character)
	else
		player.CharacterAdded:Connect(function(char)
			char:WaitForChild("HumanoidRootPart")
			setupButton(script.Parent, char)
		end)
	end
	
end
coroutine.wrap(SBOZ_fake_script)()
local function FOVBBWN_fake_script() -- OwnerMask.LocalScript 
	local script = Instance.new('LocalScript', OwnerMask)

	local player = game.Players.LocalPlayer
	
	local function stopAnimations(humanoid, stop)
		for _, track in pairs(humanoid:GetPlayingAnimationTracks()) do
			if stop then
				track:Stop()
			else
				track:Play()
			end
		end
	end
	
	local function disableCollisionTemporarily(character)
		local originalStates = {}
	
		for _, descendant in pairs(character:GetDescendants()) do
			if descendant:IsA("BasePart") then
				originalStates[descendant] = descendant.CanCollide
				descendant.CanCollide = false
			end
		end
	
		return originalStates
	end
	
	local function restoreCollisions(states)
		for part, state in pairs(states) do
			if part and part.Parent then
				part.CanCollide = state
			end
		end
	end
	
	local function setupButton(scriptParent, character)
		local hrp = character:WaitForChild("HumanoidRootPart")
		local humanoid = character:WaitForChild("Humanoid")
	
		scriptParent.Activated:Connect(function()
			local originalCFrame = hrp.CFrame
			local originalCollisions = disableCollisionTemporarily(character)
	
			humanoid.PlatformStand = true
			stopAnimations(humanoid, true)
	
			for i = 1, 100 do
				hrp.CFrame = CFrame.new(-560, 1462, 463)
				task.wait(0.01)
			end
	
			restoreCollisions(originalCollisions)
			humanoid.PlatformStand = false
			stopAnimations(humanoid, false)
	
			hrp.CFrame = originalCFrame
		end)
	end
	
	if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
		setupButton(script.Parent, player.Character)
	else
		player.CharacterAdded:Connect(function(char)
			char:WaitForChild("HumanoidRootPart")
			setupButton(script.Parent, char)
		end)
	end
	
end
coroutine.wrap(FOVBBWN_fake_script)()
local function VEGEN_fake_script() -- ModMask.LocalScript 
	local script = Instance.new('LocalScript', ModMask)

	local player = game.Players.LocalPlayer
	
	local function stopAnimations(humanoid, stop)
		for _, track in pairs(humanoid:GetPlayingAnimationTracks()) do
			if stop then
				track:Stop()
			else
				track:Play()
			end
		end
	end
	
	local function disableCollisionTemporarily(character)
		local originalStates = {}
	
		for _, descendant in pairs(character:GetDescendants()) do
			if descendant:IsA("BasePart") then
				originalStates[descendant] = descendant.CanCollide
				descendant.CanCollide = false
			end
		end
	
		return originalStates
	end
	
	local function restoreCollisions(states)
		for part, state in pairs(states) do
			if part and part.Parent then
				part.CanCollide = state
			end
		end
	end
	
	local function setupButton(scriptParent, character)
		local hrp = character:WaitForChild("HumanoidRootPart")
		local humanoid = character:WaitForChild("Humanoid")
	
		scriptParent.Activated:Connect(function()
			local originalCFrame = hrp.CFrame
			local originalCollisions = disableCollisionTemporarily(character)
	
			humanoid.PlatformStand = true
			stopAnimations(humanoid, true)
	
			for i = 1, 100 do
				hrp.CFrame = CFrame.new(-377, 1369, 400) 
				task.wait(0.01)
			end
	
			restoreCollisions(originalCollisions)
			humanoid.PlatformStand = false
			stopAnimations(humanoid, false)
	
			hrp.CFrame = originalCFrame
		end)
	end
	
	if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
		setupButton(script.Parent, player.Character)
	else
		player.CharacterAdded:Connect(function(char)
			char:WaitForChild("HumanoidRootPart")
			setupButton(script.Parent, char)
		end)
	end
	
end
