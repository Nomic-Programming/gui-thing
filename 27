local function OVTV_fake_script() -- MirrorON.LocalScript 
	local script = Instance.new('LocalScript', MirrorON)

	while true do
		if script.Parent.Visible == true then
			workspace.Vent.CanCollide = false
		else
			workspace.Vent.CanCollide = true
		end
		wait(0.3)
	end
end
